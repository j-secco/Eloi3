# UR10 Robot Server Environment Configuration

# Debug mode
DEBUG=false

# Robot connection settings
ROBOT_HOSTNAME=192.168.1.100
ROBOT_PORT=30004
RTDE_FREQUENCY=10

# Chess engine settings
STOCKFISH_PATH=/usr/bin/stockfish
STOCKFISH_DEPTH=15
STOCKFISH_TIME=1.0

# Robot parameters
MOVE_SPEED=0.3
MOVE_ACCELERATION=0.5
LIFT_HEIGHT=0.05
BOARD_HEIGHT=0.0
BOARD_LIFT_HEIGHT=0.02

# Safety limits
SPEED_MAX=0.5
Z_MIN=-0.1
Z_MAX=0.5

# Force control parameters
FORCE_SECONDS=2
FORCE_TYPE=2
TCP_DOWN=-10.0

# WebSocket settings
WS_PING_INTERVAL=20
WS_PING_TIMEOUT=10

# Session settings
SESSION_TIMEOUT=3600
MAX_CONCURRENT_SESSIONS=10

# Mock mode (for development without hardware)
MOCK_MODE=true

# CORS settings (comma-separated)
CORS_ORIGINS=http://localhost:3000,https://localhost:3000

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=

# TLS settings (optional)
TLS_CERT_FILE=
TLS_KEY_FILE=

